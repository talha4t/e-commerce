{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".int-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  },
  "testSequencer": "<rootDir>/src/custom-sequencer.ts"
}
